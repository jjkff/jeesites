<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thinkgem.jeesite.sierac.dao.ApiRdcOutDetailDao">
	<update id="update">
   	 UPDATE 
    	  rdc_out_detail 
     SET
     	 `header_id`  = #{header_id},
         `product_id` = #{product_id}, 
         `batch_code` = #{batch_code},
         `actual_pcs_qty` =#{actual_qty_pcs},
         `expect_pcs_qty` =#{expect_qty_pcs} 
         WHERE `id` = #{id} 
	</update>
</mapper>